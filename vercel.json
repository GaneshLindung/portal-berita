{
  "version": 2,
  "installCommand": "cd news-portal-frontend && npm install",
  "buildCommand": "cd news-portal-frontend && npm run build",
  "outputDirectory": "news-portal-frontend/.next",
  "builds": [
    {
      "src": "news-portal-frontend/next.config.mjs",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "news-portal-frontend/$1"
    }
  ]
}
